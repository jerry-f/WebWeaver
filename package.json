{
  "name": "newsflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3001",
    "dev:scripts": "concurrently -n worker,scheduler,ws -c blue,green,magenta \"npm:worker:dev\" \"npm:scheduler:dev\" \"npm:websocket:dev\"",
    "build": "next build",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "test:scraper": "npx tsx scripts/test-scraper.ts",
    "test:services": "npx tsx scripts/test-scraper.ts --services",
    "test:fetch": "npx tsx scripts/test-scraper.ts --fetch",
    "test:image": "npx tsx scripts/test-scraper.ts --image",
    "test:grpc": "npx tsx scripts/test-scraper.ts --grpc",
    "test:perf": "npx tsx scripts/test-scraper.ts --perf",
    "worker": "npx tsx scripts/start-workers.ts",
    "worker:dev": "npx tsx --watch scripts/start-workers.ts",
    "scheduler": "npx tsx scripts/start-scheduler.ts",
    "scheduler:dev": "npx tsx --watch scripts/start-scheduler.ts",
    "websocket": "npx tsx scripts/start-websocket.ts",
    "websocket:dev": "npx tsx --watch scripts/start-websocket.ts",
    "proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/lib/fetchers/clients --ts_proto_opt=esModuleInterop=true,outputServices=generic-definitions,useExactTypes=false --proto_path=./go-scraper-service/api/proto ./go-scraper-service/api/proto/scraper.proto && cd go-scraper-service && make proto"
  },
  "dependencies": {
